{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["dom", "es2021", "ScriptHost"],
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "allowJs": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "preserveSymlinks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "useUnknownInCatchVariables": false
  },
  "include": ["src"]
}
